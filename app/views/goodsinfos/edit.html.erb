<div class="container">
  <%= form_for :goodsinfo, url: goodsinfo_path(@goodsinfo), method: :patch do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>

     <p>
      <%= f.label :url %><br>
      <%= f.text_area :url %>
    </p>
    <p>
      <%= f.submit %>
    </p>
  <% end %>
</div>